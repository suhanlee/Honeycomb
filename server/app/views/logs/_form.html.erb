      <%= f.label :app_version %><br>
      <%= f.text_field :app_version %>
    </div>
    <div class="field">
      <%= f.label :model %><br>
      <%= f.text_field :model %>
    </div>
    <div class="field">
      <%= f.label :device %><br>
      <%= f.text_field :device %>
    </div>
    <div class="field">
      <%= f.label :contents %><br>
      <%= f.text_field :contents %>
    </div>
    <div class="actions">
      <%= f.submit %>
    </div>
<% end %>
