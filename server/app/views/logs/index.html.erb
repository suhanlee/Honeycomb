<%- model_class = Log -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th>OS</th>
      <th>App</th>
      <th>Model</th>
      <th>Code</th>
      <th>Contents</th>
      <th>Time</th>
      <th colspan="3">Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @logs.each do |log| %>
      <tr>
        <td><%= log.os_version %></td>
        <td><%= log.app_version %></td>
        <td><%= log.model %></td>
        <td><%= log.device %></td>
        <td><%= log.contents.slice(0, 20) %></td>
        <td><%= log.updated_at %></td>
        <td><%= link_to 'Show', log, :class => 'btn btn-default btn-xs' %></td>
        <td><%= link_to 'Edit', edit_log_path(log), :class => 'btn btn-default btn-xs' %></td>
        <td><%= link_to 'Delete',
                      log_path(log),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'New',
            new_log_path,
            :class => 'btn btn-primary' %>
