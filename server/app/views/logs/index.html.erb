<p id="notice"><%= notice %></p>

<h1>Listing Logs</h1>

<table>
  <thead>
    <tr>
      <th>OS</th>
      <th>App</th>
      <th>Model</th>
      <th>Code</th>
      <th>Contents</th>
      <th>Time</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @logs.each do |log| %>
      <tr>
        <td><%= log.os_version %></td>
        <td><%= log.app_version %></td>
        <td><%= log.model %></td>
        <td><%= log.device %></td>
        <td><%= log.contents.slice(0, 20) %></td>
        <td><%= log.updated_at %></td>
        <td><%= link_to 'Show', log %></td>
        <td><%= link_to 'Edit', edit_log_path(log) %></td>
        <td><%= link_to 'Destroy', log, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Log', new_log_path %>
